<h1><%= t('.title') %></h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th><%= t('.slug') %></th>
    </tr>
  </thead>
  <tbody>
  <% @pages.each do |page| %>
    <tr>
      <td><%= link_to t("admin_pages.#{page.slug}"), admin_page_path(page) %></td>
    </tr>
  <% end %>
  </tbody>
</table>
